<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.csulb.edu.customspinner" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Actress"
            android:label="@string/title_activity_actress" >
        </activity>
        <activity
            android:name=".Movies"
            android:label="@string/title_activity_movies" >
        </activity>
        <activity
            android:name=".MoviesDatabase"
            android:label="@string/title_activity_movies_database" >
        </activity>
    </application>

</manifest>
